  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p start:dev-server start:webpack",
    "start": "cross-env NODE_ENV=production npm run build:front & npm run start:prod-server",
    "lint": "eslint app --ext .js,.jsx",
    "test": "jest",
    "start:dev-server": "nodemon --exec babel-node app/server.js",
    "start:prod-server": "babel-node app/server.js",
    "start:front-server": "cross-env NODE_ENV=development webpack-dev-server",
    "start:webpack": "webpack --progress --watch --colors --config webpack.config.js",
    "build:front": "webpack --progress --colors --config webpack.config.js"
  },
